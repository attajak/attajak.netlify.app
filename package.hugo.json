{
  "name": "attajak.netlify.app",
  "version": "1.0.0",
  "private": "true",
  "description": "[![Screenshot](./.github/preview.png)](https://hugoblox.com/templates/)",
  "keywords": [
    "attajak",
    "janrak",
    "hugo",
    "blox",
    "blog",
    "netlify"
  ],
  "homepage": "https://github.com/attajak/attajak.netlify.app#readme",
  "bugs": {
    "url": "https://github.com/attajak/attajak.netlify.app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attajak/attajak.netlify.app.git"
  },
  "license": "MIT",
  "author": "attajak (https://github.com/attajak/)",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "clean": "hugo mod clean && rm -rf .hugo_build.lock _vendor/ node_modules/ resources/ public/",
    "create": "hugo new content",
    "build": "hugo --gc --minify --logLevel info && pnpm run pagefind",
    "server": "hugo server -D -E -F -M -N -O --disableFastRender --bind 0.0.0.0 -b http://localhost --logLevel info",
    "server:production": "pnpm run server -- -e production",
    "update": "pnpm update && hugo mod get -u && hugo mod tidy",
    "deps": "pnpm list && hugo mod graph",
    "start": "pnpm run server",
    "preview": "pnpm run server:production",
    "pagefind": "pagefind --site public"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "pagefind": "^1.4.0",
    "preact": "^10.28.4",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "netlify-plugin-hugo-cache-resources": "^0.2.1"
  },
  "packageManager": "pnpm@10.30.3"
}
